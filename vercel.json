{
    "regions": [
        "fra1"
    ],
    "crons": [
        {
            "path": "/api/cron/reminders",
            "schedule": "0 8 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/app/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "/app/main.py"
        }
    ]
}